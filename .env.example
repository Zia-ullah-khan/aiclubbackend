# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/aiclub_os

# Docker Configuration
# Windows: //./pipe/docker_engine
# Linux: /var/run/docker.sock
DOCKER_SOCKET_PATH=//./pipe/docker_engine

# Local Storage Root for user files
# Windows: C:/Projects/aiclub_data/
# Linux: /home/yourusername/aiclub_data/
LOCAL_STORAGE_ROOT=C:/Projects/aiclub_data/

# JWT Secret for authentication
JWT_SECRET=changeme_to_something_secure

# Server Configuration
PORT=3001
NODE_ENV=development

# Session Configuration
SESSION_SECRET=your_session_secret_here
